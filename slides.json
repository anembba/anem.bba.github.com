layout: null
---

{
    {% assign slides = site.static_files | where:"extname",".jpg"   %}

  "slides": [
    {% for file in slides %}
      
        "path": "{{ file.path }}",
        "modified_time": "{{ file.modified_time  }}",
        "extname": "{{ file.extname }}"
      }
      {% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}