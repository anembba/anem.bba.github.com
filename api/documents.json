---
layout: null
---

{
    
    {% assign docs = site.static_files | where_exp:"item" ,"item.extname contains  '.doc'||'.xlsx' "   %}
  
  "total": {{docs | size }},
  "docs": [
    {% for file in docs %}
      {
        "index": {{forloop.index0}},
        "path": "{{ file.path }}",
        "url": "{{ file.path | prepend: site.url }}",
        "modified_time": "{{ file.modified_time  }}",
        "extname": "{{ file.extname }}"
      }
      {% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}